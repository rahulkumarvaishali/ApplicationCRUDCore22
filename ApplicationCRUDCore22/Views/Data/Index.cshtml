@model IEnumerable<AppDbContext>
<h1>Data Table</h1>

<table class="table table-bordered">
    <tr>
        <th>id</th>
        <th>name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Address</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.id</td>
            <td>@item.name</td>
            <td>@item.Email</td>
            <td>@item.Mobile</td>
            <td>@item.Address</td>
        </tr>
    }


</table>
